    <fragment
        android:id="@+id/doneFragment"
        android:name="com.md.williamriesen.hawkeyeharvestfoodbank.DoneFragment"
        android:label="DoneFragment" >
        <action
            android:id="@+id/action_doneFragment_to_clientStartFragment"
            app:destination="@id/clientStartFragment" />
    </fragment>
    <fragment
        android:id="@+id/clientStartFragment"
        android:name="com.md.williamriesen.hawkeyeharvestfoodbank.ClientStartFragment"
        android:label="fragment_client_start"
        tools:layout="@layout/fragment_client_start" >
        <action
            android:id="@+id/action_clientStartFragment_to_selectionFragment"
            app:destination="@id/selectionFragment"
            app:popUpTo="@id/clientStartFragment" />
